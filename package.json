{
  "name": "resonate-search",
  "version": "1.0.0",
  "description": "Resonate Elastic Search Service",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:lint src && npm run test:deps",
    "test:lint": "standard src",
    "test:deps": "dependency-check --verbose --missing ./lib/**/*.js --no-dev --i mysql2 --i dotenv-safe --i sequelize-cli",
    "dev": "babel-node -r ./node_modules/dotenv-safe/config ./src/server.js",
    "start": "node -r ./node_modules/dotenv-safe/config ./lib/server.js",
    "build": "babel src -d lib"
  },
  "keywords": [
    "elasticsearch",
    "mongodb",
    "koa"
  ],
  "author": "Augustin Godiscal <auggod@resonate.is>",
  "license": "MIT",
  "dependencies": {
    "@koa/router": "^8.0.8",
    "ajv": "^6.12.2",
    "bluebird": "^3.7.2",
    "decode-uri-component": "^0.2.0",
    "dotenv-safe": "^8.2.0",
    "elasticsearch": "^16.7.1",
    "ioredis": "^4.16.3",
    "keygrip": "^1.1.0",
    "koa": "^2.11.0",
    "koa-compress": "^3.1.0",
    "koa-json-error": "^3.1.2",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-ratelimit": "^4.2.1",
    "koa-session": "^5.13.1",
    "mongoosastic": "^4.6.0",
    "mongoose": "^5.10.9",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "babel-plugin-add-module-exports": "^1.0.2",
    "dependency-check": "^4.1.0",
    "mysql2": "^2.1.0",
    "sequelize": "^6.19.2",
    "sequelize-cli": "^5.5.1",
    "standard": "^17.0.0",
    "tape": "^4.13.2",
    "yargs": "^15.3.1"
  }
}
